menu 'cpufreq'
    config TINATEST_STRESS_CPU_CPUFREQ_ENABLE
        bool "Enable"
        default n

    if TINATEST_STRESS_CPU_CPUFREQ_ENABLE
        config TINATEST_STRESS_CPU_CPUFREQ_COMMAND
            string "command"
            default "cpufreq.sh 10000 2 linux"

        config TINATEST_STRESS_CPU_CPUFREQ_STDIN
            string "stdin"
            default ""

        config TINATEST_STRESS_CPU_CPUFREQ_FSTDIN
            string "fstdin"
            default ""

        comment "<N> to Default && <Y> to customize"
        config TINATEST_STRESS_CPU_CPUFREQ_INFO
            bool "INFO"
            default y

        if TINATEST_STRESS_CPU_CPUFREQ_INFO
            config TINATEST_STRESS_CPU_CPUFREQ_DATE
                bool "date"
                default y if TINATEST_SYS_LOCAL_INFO_DATE

            config TINATEST_STRESS_CPU_CPUFREQ_RESOURCE
                bool "resource"
                default y if TINATEST_SYS_LOCAL_INFO_RESOURCE
        endif

        comment "<N> to Default && <Y> to customize"
        config TINATEST_STRESS_CPU_CPUFREQ_LIMIT
            bool "LIMIT"
            default y

        if TINATEST_STRESS_CPU_CPUFREQ_LIMIT
            config TINATEST_STRESS_CPU_CPUFREQ_RUN_TIMES
                string "run_times"
                default 1

            config TINATEST_STRESS_CPU_CPUFREQ_RUN_ALONE
                bool "run_alone"
                default y

            config TINATEST_STRESS_CPU_CPUFREQ_RUN_PARALLEL
                bool "run_parallel"
                default flase

            config TINATEST_STRESS_CPU_CPUFREQ_MAY_REBOOT
                bool "may_reboot"
                default y if TINATEST_SYS_LOCAL_LIMIT_MAY_REBOOT

            config TINATEST_STRESS_CPU_CPUFREQ_RUN_TIME_SEC
                string "run_time_sec"
                default "Default"

            config TINATEST_STRESS_CPU_CPUFREQ_RUN_TIME_MIN
                string "run_time_min"
                default "Default"

            config TINATEST_STRESS_CPU_CPUFREQ_RUN_TIME_HOUR
                string "run_time_hour"
                default "Default"
        endif
    endif
endmenu
